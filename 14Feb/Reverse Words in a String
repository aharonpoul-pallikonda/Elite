class Solution {
    public String reverseWords(String s) {
         String[] strArray = s.split(" ");
        String ans = "";
        for(int i = strArray.length - 1; i >= 0 ; i--){
            if(strArray[i] == ""){
                continue;
            }
            ans = ans + " " + strArray[i];
        }

        return ans.trim();
    }
}
